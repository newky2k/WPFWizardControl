<Project Sdk="Microsoft.NET.Sdk.WindowsDesktop">
  <PropertyGroup>
    <TargetFramework>net461</TargetFramework>
    <UseWPF>true</UseWPF>
    <GenerateAssemblyInfo>true</GenerateAssemblyInfo>
    <GeneratePackageOnBuild>true</GeneratePackageOnBuild>
    <PackageRequireLicenseAcceptance>true</PackageRequireLicenseAcceptance>
    <PackageId>DSoft.WizardControl.WPF</PackageId>
    <Authors>newky2k</Authors>
    <PackageLicenseExpression>MIT</PackageLicenseExpression>
    <SignAssembly>true</SignAssembly>
    <AssemblyOriginatorKeyFile>DSoft.snk</AssemblyOriginatorKeyFile>
    <Version>1.3.0-preview1</Version>
    <Description>DSoft.WizardControl.WPF is a simple wizard user control for WPF</Description>
    <PackageProjectUrl>https://github.com/newky2k/WPFWizardControl</PackageProjectUrl>
    <RepositoryUrl>https://github.com/newky2k/WPFWizardControl</RepositoryUrl>
    <RepositoryType>Git</RepositoryType>
    <PackageTags>WPF Wizard</PackageTags>
    <PackageReleaseNotes>Added .NET Core WPF support</PackageReleaseNotes>
    <Copyright>Copyright © 2019 DSoft</Copyright>
  </PropertyGroup>
  <ItemGroup>
    <PackageReference Include="Microsoft.Windows.Compatibility" Version="2.1.1" />
    <PackageReference Include="System.Mvvm" Version="1.1.2" />
  </ItemGroup>
</Project>